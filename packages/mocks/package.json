{
    "name": "@redux-things/mocks",
    "version": "1.5.0",
    "description": "A library that allows you to quickly and easily synchronize your store with the server.",
    "keywords": [
        "redux",
        "queries",
        "things",
        "data",
        "data management",
        "caching",
        "cache",
        "fetch",
        "promise",
        "store",
        "state"
    ],
    "main": "dist/index.js",
    "scripts": {
        "build": "node build.js",
        "watch": "watch 'npm run build' src",
        "lint": "npx eslint --fix --ignore-path ../../.gitignore ."
    },
    "author": "Eugen Guriev",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "babel": {
        "plugins": [
            [
                "module-resolver",
                {
                    "root": [
                        "./src"
                    ],
                    "alias": {
                        "@": "./src"
                    }
                }
            ],
            "dynamic-import-node",
            "add-module-exports"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "chrome": 78,
                        "edge": 15,
                        "esmodules": false
                    }
                }
            ],
            "@babel/preset-react"
        ]
    },
    "dependencies": {
        "@redux-things/core": "^1.5.0",
        "@reduxjs/toolkit": "^1.5.0",
        "core-js": "^2.6.12",
        "object-hash": "^2.1.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-transform-modules-commonjs": "^7.13.8",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@storybook/addon-actions": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-links": "^6.1.21",
        "@storybook/react": "^6.1.21",
        "@svgr/webpack": "^5.4.0",
        "@testing-library/react-hooks": "^5.1.2",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.1.0",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-module-resolver": "^4.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-dir": "^1.3.0",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.23.2",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.2",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.5",
        "watch": "^1.0.2"
    },
    "peerDependencies": {
        "object-hash": "^2.1.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-redux": "^7.2.2",
        "react-use": "^17.1.1"
    },
    "bugs": {
        "url": "https://github.com/theguriev/redux-things/issues"
    },
    "homepage": "https://github.com/theguriev/redux-things",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theguriev/redux-things.git"
    },
    "gitHead": "046bdecff9e5080cdd59c39aac61e3052588f3e1"
}
