{
    "name": "@redux-things/storybook",
    "version": "1.6.2",
    "description": "A library that allows you to quickly and easily synchronize your store with the server",
    "scripts": {
        "start": "start-storybook -p 6006 --no-manager-cache",
        "storybook": "start-storybook -p 6006",
        "storybook-docs": "start-storybook --docs --no-manager-cache",
        "build": "build-storybook --docs -o ../../docs"
    },
    "author": "Eugen Guriev",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "babel": {
        "plugins": [
            [
                "module-resolver",
                {
                    "root": [
                        "./src"
                    ],
                    "alias": {
                        "@": "./src"
                    }
                }
            ],
            "dynamic-import-node",
            "add-module-exports"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "chrome": 78,
                        "edge": 15,
                        "esmodules": false
                    }
                }
            ],
            "@babel/preset-react"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-links": "^6.1.21",
        "@storybook/react": "^6.1.21"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-query": "^2.25.2",
        "react-use": "^17.1.1",
        "redux": "^4.1.0"
    },
    "dependencies": {
        "@cryptic-css/core": "^5.0.3",
        "@redux-things/core": "^1.6.2",
        "@you-eye/styled": "^5.0.3",
        "styled-components": "^5.3.0"
    },
    "gitHead": "046bdecff9e5080cdd59c39aac61e3052588f3e1"
}
